<link rel="stylesheet" href="/style/user/home.css">
<div class="outer-container">
    <div class="head-container">
        <h1>USER DASHBOARD</h1>
        <h4 class="text-success"><%= msg %></h4>
        <form action="/user/logout" method="post">
           <input type="submit" value="logout" class="btn btn-danger">
        </form>
    </div>
    <h4 class="text-center text-secondary">Pending Topics</h4>
    <% pendingTopicsofCurrentUser.forEach(element=> { %>
        <div class="topics-container container">
            <p><%= element %></p>
        </div>
    <% }) %>
    
</div>